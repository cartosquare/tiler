			"data_source": "osm",
			"data_name": "water_areas",
			"encode": "utf-8",
			"geometry_column": "way",
			"fields": "*",
			"rules": [
				{
					"res_max": "@z6",
					"res_min": "@z7",
					"data_filter": "[natural] = 'glacier'",
					"fill_image": "@water-area_glacier-lowz",
					"outline_color": "@water-area_glacier-lowz",
					"outline_width": 1.5,
					"outline_dasharray": [
						4,
						2
					]
				},
				{
					"res_max": "@z8",
					"res_min": "@z19",
