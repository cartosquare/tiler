{
	"layers": {
		"water-lines-casing": {
			"data_source": "osm",
			"data_name": "water_lines_casing",
			"encode": "utf-8",
			"geometry_column": "way",
			"fields": "*",
			"rules": [
				{
					"res_max": "@z13",
					"res_min": "@z14",
					"data_filter": "[waterway] = 'stream' or [waterway] = 'ditch' or [waterway] = 'drain'",
					"line_color": "@water-lines-casing",
					"line_width": 1.5
				},
				{
					"res_max": "@z15",
					"res_min": "@z19",
					"data_filter": "[waterway] = 'stream'",
					"line_color": "@water-lines-casing",
					"line_width": 2.5
				}
			]
		},
